{
  "name": "linter-psalm",
  "main": "./lib/main.js",
  "private": true,
  "version": "0.1.2",
  "description": "Lint PHP on the fly, using psalm",
  "repository": "https://github.com/albert200000/linter-psalm",
  "license": "MIT",
  "configSchema": {
    "executablePath": {
      "type": "string",
      "title": "Psalm Executable Path",
      "default": "psalm"
    }
  },
  "dependencies": {
    "atom-linter": "10.0.0",
    "atom-package-deps": "5.1.0"
  },
  "engines": {
    "atom": ">=1.7.0 <2.0.0"
  },
  "package-deps": [
    "linter: 2.0.0"
  ],
  "activationHooks": [
    "language-php:grammar-used"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  }
}
